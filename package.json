{
  "name": "web-scraper",
  "version": "1.0.1",
  "description": "A simple API that allows you to scrape a webpage.  You pass (POST) it a URL to scrape and a selector to find on that page.",
  "main": "app.js",
  "scripts": {
    "start": "env DEBUG=scrape:* nodemon ./bin/www",
    "prod": "node ./bin/www"
  },
  "dependencies": {
    "body-parser": "^1.12.2",
    "cheerio": "^0.22.0",
    "debug": "^2.6.0",
    "express": "^4.12.3",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "request": "^2.55.0"
  },
  "engines": {
    "node": "7.4.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvilrokx/universal-web-scraper"
  },
  "keywords": [
    "html scraper",
    "webpage scraper",
    "node",
    "express"
  ],
  "license": "MIT"
}
